{
    "version": "1.0.0-*",
    "description": "ASP.NET 5 runtime infrastructure for assembly load contexts.",
    "dependencies": {
        "Microsoft.Dnx.Runtime.Abstractions": "1.0.0-*"
    },

    "frameworks": {
        "dnx451": {
            "dependencies": {
            }
        },
        "dnxcore50": {
            "dependencies": {
                "System.Collections": "4.0.11-beta-*",
                "System.Collections.Concurrent": "4.0.11-beta-*",
                "System.Runtime.Loader": "4.0.0-beta-*",
                "System.IO.FileSystem": "4.0.1-beta-*",
                "System.AppContext": "4.0.1-beta-*",
                "System.Threading": "4.0.11-beta-*"
            }
        }
    },
    "scripts": {
        "postbuild": [
            "%project:Directory%/../../build/batchcopy \"%project:BuildOutputDir%/%build:Configuration%/%build:TargetFramework%/*.*\" %project:Directory%/../../artifacts/build/dnx-clr-win-x86/bin/lib",
            "%project:Directory%/../../build/batchcopy \"%project:BuildOutputDir%/%build:Configuration%/%build:TargetFramework%/*.*\" %project:Directory%/../../artifacts/build/dnx-clr-win-x64/bin/lib",
            "%project:Directory%/../../build/batchcopy \"%project:BuildOutputDir%/%build:Configuration%/%build:TargetFramework%/*.*\" %project:Directory%/../../artifacts/build/dnx-mono/bin"
        ]
    }
}

